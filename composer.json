{
    "name": "ali/design-pattern",
    "description": "Code expamples for Design Patterns in PHP",
    "autoload": {
        "psr-4": {
            "Ali\\DesignPattern\\": "src/",
            "Creational\\": "src/Creational/",
            "Structural\\": "src/Structural/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "repositories": [
        {
            "type": "path",
            "url": "Packages/BasicAuthLib"
        },
        {
            "type": "path",
            "url": "Packages/TokenAuthLib"
        }
    ],
    "require": {
        "ali.ahmed/token-auth-lib": "*@dev",
        "ali.ahmed/basic-auth-lib": "*@dev"
    }
}
